feature nxapi
!
nxapi http port 80
nxapi https port 443
! Switchname
switchname {{ inventory_hostname }}
! Vagrant Credentials
username vagrant password 0 vagrant role network-admin
username vagrant shelltype vsh
username vagrant sshkey ssh-rsa  vagrant insecure public key
! Management Inband Configuration
! Reset interface's mac address
interface ethernet 1/1-64
  shut
  mac-address 1234.4321.1234
  no mac-address
  no shut
interface {{ inbound_mgmt_interface }}
  no switchport
  no ip redirects
  ip address {{ inbound_mgmt_ip }} {{ inbound_mgmt_netmask }}
  no shutdown
  exit
end
